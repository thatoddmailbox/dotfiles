alias d='doc'
alias down='dow'
alias m='mkdir -p'
alias o='xo'
alias s='ssh'
alias sch='school'
alias x='xo'

c() {
	if [[ -e $1 ]]; then
		cd $1
	elif [[ $1 ]]; then
		cd *$1*
	fi
}
e() {
	if [[ $1 ]]; then
		echo *$1*
	fi
}
mkc() {
	mkdir -p $1
	cd $1
}
xo() {
	if [[ $1 ]]; then
		xdg-open $1
	else
		xdg-open .
	fi
}

doc() {
	cd ~/Documents
	c $1
}
